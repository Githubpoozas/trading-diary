{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "ticket": {
      "type": "string",
      "unique": false
    },
    "openTime": {
      "type": "datetime"
    },
    "type": {
      "type": "string",
      "regex": "\\b(sell|buy|sellLimit|buyLimit|sellStop|buyStop)\\b"
    },
    "size": {
      "type": "decimal"
    },
    "openPrice": {
      "type": "float"
    },
    "closeTime": {
      "type": "datetime"
    },
    "profit": {
      "type": "decimal"
    },
    "order_changes": {
      "via": "order",
      "collection": "order-change"
    },
    "comment": {
      "type": "text"
    },
    "trade": {
      "via": "orders",
      "model": "trade"
    },
    "closePrice": {
      "type": "float"
    },
    "swap": {
      "type": "decimal"
    },
    "product": {
      "model": "product",
      "via": "orders"
    },
    "open": {
      "type": "boolean",
      "default": true
    },
    "stopLoss": {
      "type": "float"
    },
    "takeProfit": {
      "type": "float"
    }
  }
}
