      "regex": "\\b(xm)\\b"
    },
    "type": {
      "type": "string",
      "regex": "\\b(forex|metals|indices|crypto)\\b",
      "required": true
    },
    "digits": {
      "type": "integer"
    },
    "contractSize": {
      "type": "integer"
    }
  }
}
